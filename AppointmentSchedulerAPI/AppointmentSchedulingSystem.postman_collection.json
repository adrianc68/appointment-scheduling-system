							"listen": "prerequest",
							"script": {
								"exec": [
									"const Allowed = {",
									"    Uppers: \"QWERTYUIOPASDFGHJKLZXCVBNM\",",
									"    Lowers: \"qwertyuiopasdfghjklzxcvbnm\",",
									"    Numbers: \"1234567890\",",
									"    Symbols: \"!@#$%^&*\"",
									"};",
									"",
									"const getRandomCharFromString = (str) => str.charAt(Math.floor(Math.random() * str.length));",
									"",
									"const generatePassword = (length = 8) => {",
									"    let pwd = \"\";",
									"    pwd += getRandomCharFromString(Allowed.Uppers); // At least one uppercase",
									"    pwd += getRandomCharFromString(Allowed.Lowers); // At least one lowercase",
									"    pwd += getRandomCharFromString(Allowed.Numbers); // At least one number",
									"    pwd += getRandomCharFromString(Allowed.Symbols); // At least one symbol",
									"",
									"    for (let i = pwd.length; i < length; i++) {",
									"        pwd += getRandomCharFromString(Object.values(Allowed).join(''));",
									"    }",
									"    return pwd;",
									"}",
									"",
									"",
									"const password = generatePassword(12);",
									"pm.environment.set(\"randomPassword\", password);",
									"",
									"const getRandomNumber = (n) => {",
									"    let result = '';",
									"    for (let i = 0; i < n; i++) {",
									"        result += Math.floor(Math.random() * 10);",
									"    }",
									"    return result;",
									"};",
									"",
									"const areaCode = \"55\";",
									"const phoneNumber = getRandomNumber(10);",
									"",
									"pm.environment.set(\"randomPhoneNumber\", areaCode + phoneNumber);"
